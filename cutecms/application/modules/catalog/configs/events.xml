<?xml version="1.0"?>
<config>
    <modules>
		<kernel/>
		<checkout/>
    </modules>
    <events>
    
        
        <Catalog_AdminController__init>
            <observers>
                <kernelInit>
                    <class>Observer_AdminController</class>
                    <method>initAction</method>
                </kernelInit>
                <selfInit>
                    <class>Catalog_Observer_AdminController</class>
                    <method>initAction</method>
                </selfInit>
            </observers>
        </Catalog_AdminController__init>
        
        <Catalog_Controller__init>
            <observers>
                <kernelInit>
                    <class>Observer_Controller</class>
                    <method>initAction</method>
                </kernelInit>
                <selfInit>
                    <class>Catalog_Observer_Controller</class>
                    <method>initAction</method>
                </selfInit>
            </observers>
        </Catalog_Controller__init>
        
        
        
        
        

        <Catalog_AdminItem_IndexController__editAction__onLoad>
            <observers>
                <index>
                    <class>Catalog_Observer_AdminItem_Index</class>
                    <method>editOnLoad</method>
                </index>
                <attribute>
                    <class>Catalog_Observer_AdminItem_Attribute</class>
                    <method>editOnLoad</method>
                </attribute>
                <bundle>
                    <class>Catalog_Observer_AdminItem_Bundle</class>
                    <method>editOnLoad</method>
                </bundle>
                <xsell>
                    <class>Catalog_Observer_AdminItem_Xsell</class>
                    <method>editOnLoad</method>
                </xsell>
            </observers>
        </Catalog_AdminItem_IndexController__editAction__onLoad>

        <Catalog_AdminItem_IndexController__editAction__onSave>
            <observers>
                <index>
                    <class>Catalog_Observer_AdminItem_Index</class>
                    <method>editOnSave</method>
                </index>
                <attribute>
                    <class>Catalog_Observer_AdminItem_Attribute</class>
                    <method>editOnSave</method>
                </attribute>
                <bundle>
                    <class>Catalog_Observer_AdminItem_Bundle</class>
                    <method>editOnSave</method>
                </bundle>
                <xsell>
                    <class>Catalog_Observer_AdminItem_Xsell</class>
                    <method>editOnSave</method>
                </xsell>
            </observers>
        </Catalog_AdminItem_IndexController__editAction__onSave>

        <Catalog_AdminItem_IndexController__editAction__onAfterSuccessSave>
            <observers>
                <bundle>
                    <class>Catalog_Observer_AdminItem_Bundle</class>
                    <method>editOnAfterSave</method>
                </bundle>
                <xsell>
                    <class>Catalog_Observer_AdminItem_Xsell</class>
                    <method>editOnAfterSave</method>
                </xsell>
            </observers>
        </Catalog_AdminItem_IndexController__editAction__onAfterSuccessSave>

        
        

        <Catalog_Model_Object_Attribute__trigger__onBeforeSave>
        	<observers>
        		<debug>
        			<class>Catalog_Model_Object_Observer_Debug</class>
        			<method>onBeforeSaveAttribute</method>
        		</debug>
        	</observers>
        </Catalog_Model_Object_Attribute__trigger__onBeforeSave>

        <Catalog_Model_Object_Item__trigger__onBeforeDelete>
            <observers>
                <seoId>
                    <class>Catalog_Model_Object_Observer_Item</class>
                    <method>onBeforeDelete</method>
                </seoId>
            </observers>
        </Catalog_Model_Object_Item__trigger__onBeforeDelete>

        <Catalog_Model_Object_Item__trigger__onAfterSave>
            <observers>
                <seoId>
                    <class>Catalog_Model_Object_Observer_Item</class>
                    <method>onAfterSave</method>
                </seoId>
            </observers>
        </Catalog_Model_Object_Item__trigger__onAfterSave>

        <Catalog_Model_Object_Attribute__trigger__onAfterSave>
            <observers>
                <seoId>
                    <class>Catalog_Model_Object_Observer_ItemSearch</class>
                    <method>onAfterSave</method>
                </seoId>
            </observers>
        </Catalog_Model_Object_Attribute__trigger__onAfterSave>

        <Catalog_Model_Object_Attribute__trigger__onAfterDelete>
            <observers>
                <seoId>
                    <class>Catalog_Model_Object_Observer_ItemSearch</class>
                    <method>onAfterDelete</method>
                </seoId>
            </observers>
        </Catalog_Model_Object_Attribute__trigger__onAfterDelete>

        <Model_Collection_Interface__afterDeleteCollection>
            <observers>
                <seoId>
                    <class>Catalog_Model_Object_Observer_ItemSearch</class>
                    <method>afterDeleteCollection</method>
                </seoId>
            </observers>
        </Model_Collection_Interface__afterDeleteCollection>

