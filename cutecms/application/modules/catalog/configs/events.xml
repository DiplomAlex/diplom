        	<observers>
        		<seoId>
        			<class>Catalog_Model_Object_Observer_SeoId</class>
        			<method>onBeforeSave</method>
        		</seoId>
        	</observers>
        </Catalog_Model_Object_Category__trigger__onBeforeSave>

        <Catalog_Model_Object_Item__trigger__onBeforeSave>
        	<observers>
        		<seoId>
        			<class>Catalog_Model_Object_Observer_SeoId</class>
        			<method>onBeforeSave</method>
        		</seoId>
        	</observers>
        </Catalog_Model_Object_Item__trigger__onBeforeSave -->


<!--  checkout connection -->
		<Catalog_Model_Service_Item__onAddToCart>
        	<observers>
        		<socket>
        			<class>Catalog_Model_Service_Observer_Item</class>
        			<method>onAddToCart</method>
        		</socket>
        	</observers>
		</Catalog_Model_Service_Item__onAddToCart>

        <Catalog_Model_Service_Item__onItemToArray>
            <observers>
                <socket>
                    <class>Catalog_Model_Service_Observer_Item</class>
                    <method>onItemToArray</method>
                </socket>
            </observers>
        </Catalog_Model_Service_Item__onItemToArray>

		<Checkout_Model_Service_Cart__onAddToCart>
        	<observers>
        		<socket>
        			<module>checkout</module>
        			<class>Checkout_Model_Service_Observer_Cart</class>
        			<method>onAddToCart</method>
        		</socket>
        	</observers>
		</Checkout_Model_Service_Cart__onAddToCart>

    </events>
</config>
